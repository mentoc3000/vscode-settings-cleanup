{
  "name": "vscode-settings-organizer",
  "displayName": "Settings.json Organizer",
  "description": "Organize VS Code settings.json files",
  "publisher": "mentoc3000",
  "icon": "images/icon.png",
  "keywords": [
    "settings",
    "organizer",
    "json",
    "vscode"
  ],
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "license": "MIT",
  "homepage": "https://github.com/mentoc3000/vscode-settings-cleanup/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mentoc3000/vscode-settings-cleanup"
  },
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "settings-organizer.organize",
        "title": "Organize VS Code Settings",
        "category": "Settings",
        "icon": {
          "light": "images/spray-light.svg",
          "dark": "images/spray-dark.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "settings-organizer.organize",
          "when": "resourceFilename =~ /settings.json/ && !editorReadonly",
          "group": "7_modification"
        }
      ],
      "editor/title/run": [
        {
          "command": "settings-organizer.organize",
          "when": "resourceFilename =~ /settings.json/ && !editorReadonly",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "json5": "^2.2.3"
  }
}
